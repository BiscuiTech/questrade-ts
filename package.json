{
  "name": "questrade-ts",
  "version": "1.0.0",
  "description": "Unofficial Questrade API wraper with typescript support by Luxcium.",
  "main": "./build/index.js",
  "types": "./build/lib/index.d.ts",
  "scripts": {
    "smoke:test": "jest --silent --detectOpenHandles --forceExit --config ./src/__tests__/smoketest/jest.config.js",
    "build": "tsc --declarationDir ./build/lib/",
    "build:diagnostics": "tsc --declarationDir ./build/lib/ --diagnostics",
    "build:traceResolution": "tsc --declarationDir ./build/lib/ --traceResolution",
    "build:listFiles": "tsc --declarationDir ./build/lib/ --listFiles",
    "build:listEmittedFiles": "tsc --declarationDir ./build/lib/ --listEmittedFiles",
    "predist": "rimraf build;npm run smoke:test",
    "dist": "npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/luxcium/questrade.git"
  },
  "keywords": [
    "Questrade",
    "stocks",
    "trading",
    "typescript"
  ],
  "author": "Luxcium <luxcium@neb401.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luxcium/questrade/issues"
  },
  "homepage": "https://github.com/luxcium/questrade",
  "dependencies": {
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "moment": "^2.20.1",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/mkdirp": "^0.5.2",
    "@types/moment": "^2.13.0",
    "@types/node": "^11.13.0",
    "@types/request": "^2.48.1",
    "chai": "^4.2.0",
    "jest": "^24.7.1",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.1",
    "tslib": "^1.9.3",
    "tslint": "^5.15.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.1",
    "typescript-tslint-plugin": "^0.3.1"
  }
}
