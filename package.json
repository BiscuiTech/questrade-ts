{
  "name": "questrade-ts",
  "displayName": "Questrade API by Luxcium (with typescript support).",
  "version": "0.8.0",
  "description": "Unofficial Questrade API wrapper for NodeJS with full TypeScript support.",
  "main": "./src/api/index.js",
  "types": "./lib/api/index.d.ts",
  "scripts": {
    "build": "yarn run build:yarn",
    "build:yarn": "yarn run out:clean && yarn run quick:yarn",
    "build:npm": "npm run out:clean && npm run quick:npm",
    "quick": "yarn run quick:yarn",
    "quick:yarn": "yarn run test:yarn && yarn run rebuild:yarn && yarn run out:clean:post && yarn run out:clean:umd ",
    "quick:npm": "npm run test:npm && npm run rebuild:npm && npm run out:clean:post && npm run out:clean:umd ",
    "rebuild": "yarn run rebuild:yarn",
    "rebuild:yarn": "concurrently \"yarn:out:sys\" \"yarn:out:amd\" \"yarn:out:cjs\" \"yarn:out:umd\"",
    "rebuild:npm": "concurrently \"npm:out:sys\" \"npm:out:amd\" \"npm:out:cjs\" \"npm:out:umd\"",
    "out:clean": "rimraf -r ./build/**/*",
    "out:clean:post": "rimraf -r ./build/src/api/core/typescript/**/*",
    "out:clean:umd": "rimraf -r ./build/js/umd/src/api/core/typescript/**/*",
    "out:amd": "tsc -b ./tsconfig.amd.json",
    "out:sys": "tsc -b ./tsconfig.sys.json",
    "out:cjs": "tsc -b ./tsconfig.commonjs.json",
    "out:umd": "tsc -b ./tsconfig.umd.json",
    "out:enu": "npm run out:enu:npm",
    "out:enu:yarn": "tsc -b ./src/enums/tsconfig.json && yarn run cp:enu:yarn",
    "out:enu:npm": "tsc -b ./src/enums/tsconfig.json && npm run cp:enu:npm",
    "cp:enu": "npm run cp:enu:npm",
    "cp:enu:yarn": "concurrently \"yarn:cp:enu:0\" \"yarn:cp:enu:1\" \"yarn:cp:enu:2\" \"yarn:cp:enu:3\"",
    "cp:enu:npm": "concurrently \"npm:cp:enu:0\" \"npm:cp:enu:1\" \"npm:cp:enu:2\" \"npm:cp:enu:3\"",
    "cp:enu:0": "cp-cli ./enums/LICENSE.txt ./build/enums/LICENSE.txt",
    "cp:enu:1": "cp-cli ./enums/README.md ./build/enums/README.md",
    "cp:enu:2": "cp-cli ./enums/package.json ./build/enums/package.json",
    "cp:enu:3": "cp-cli ./enums/.npmignore ./build/enums/.npmignore",
    "lint": "tslint --project . --format stylish",
    "pretest": "npm run lint && tsc --noEmit --pretty",
    "pretest:yarn": "yarn run lint && tsc --noEmit --pretty",
    "pretest:npm": "npm run lint && tsc --noEmit --pretty",
    "test": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "coveralls":"jest --coverage --coverageReporters=text-lcov | coveralls",
    "coverage": "nyc npm run test",
    "test:yarn": "jest",
    "test:npm": "jest",
    "npm:enum:publish": " npm publish ./build/enums/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/luxcium/questrade-ts.git"
  },
  "keywords": [
    "Questrade",
    "stocks",
    "trading",
    "typescript"
  ],
  "author": "Luxcium <luxcium@neb401.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luxcium/questrade-ts/issues"
  },
  "homepage": "https://github.com/luxcium/questrade-ts",
  "dependencies": {
    "axios": "0.19.0",
    "http-status-codes": "^1.3.2",
    "pnpm": "^3.8.1",
    "questrade-api-enumerations": "^1.1.3",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/parser": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/types": "^7.6.1",
    "@types/jasmine": "^3.4.1",
    "@types/jest": "^24.0.18",
    "@types/node": "12.6.8",
    "babel-jest": "^24.9.0",
    "concurrently": "4.1.2",
    "coveralls": "^3.0.6",
    "cp-cli": "2.0.0",
    "jest": "^24.9.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "rimraf": "2.6.3",
    "ts-jest": "^24.1.0",
    "ts-node": "8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-security": "^1.16.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.3",
    "typescript-tslint-plugin": "0.5.4"
  }
}
