    "url": "https://github.com/luxcium/questrade/issues"
  },
  "homepage": "https://github.com/luxcium/questrade",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/lodash": "^4.14.123",
    "@types/mkdirp": "^0.5.2",
    "@types/moment": "^2.13.0",
    "axios": "^0.18.0",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@types/node": "^11.13.0",
    "cp-cli": "^2.0.0",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "ts-node": "^8.0.3",
    "tslib": "^1.9.3",
    "tslint": "^5.15.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.2",
    "typescript-tslint-plugin": "^0.3.1"
  }
}
