    "no-path-concat": "off", // (no replacement)
    "no-process-env": "off", // (no replacement)
    "no-process-exit": "off", // (no replacement)
    "no-restricted-modules": "off", // (no replacement)
    "no-spaced-func": "off", //
    "no-sync": "off", // (no replacement)
    "prefer-reflect": "off", // (no replacement)
    "require-jsdoc": "off", // (no replacement)
    "valid-jsdoc": "off", // (no replacement),
    // Replaced by
    "id-denylist": "off",
    // "indent": [
    //   "warn","error",{"MemberExpression":1}, { "offsetTernaryExpressions": true }], // , {"ignoredNodes": ["ConditionalExpression"] }
    "no-else-return": "warn",
    "padding-line-between-statements": [
      "warn",
      {
        "blankLine": "always",
        "prev": "*",
        "next": "return"
      },
      {
        "blankLine": "always",
        "prev": [
          "block-like",
          "multiline-block-like",
          "function"
        ],
        "next": [
          "*"
        ]
      },
      {
        "blankLine": "never",
        "prev": [
          "singleline-const",
          "singleline-let",
          "singleline-var"
        ],
        "next": [
          "const",
          "let",
          "var"
        ]
      },
      {
        "blankLine": "always",
        "prev": [
          "multiline-const",
          "multiline-let",
          "multiline-var"
        ],
        "next": [
          "*"
        ]
      }
    ],
    "no-shadow": "error",
    "no-unsafe-negation": "error",
    "func-call-spacing": "warn",
    "no-global-assign": "error",
    /*
