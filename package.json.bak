{
  "name": "questrade-ts",
  "displayName": "Questrade API by Luxcium (with typescript support).",
  "version": "0.7.0",
  "description": "Unofficial Questrade API wraper by Luxcium (with typescript support).",
  "main": "./src/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "out:clean": "rimraf -r ./build/**/*",
    "out:amd": "tsc -b ./tsconfig.amd.json",
    "out:sys": "tsc -b ./tsconfig.sys.json",
    "out:cjs": "tsc -b ./tsconfig.commonjs.json",
    "out:umd": "tsc -b ./tsconfig.umd.json",
    "rebuild": "concurrently \"yarn:out:sys\" \"yarn:out:amd\" \"yarn:out:cjs\" \"yarn:out:umd\"",
    "build": "yarn run out:clean; yarn run rebuild",
    "smoke:test": "jest --silent --detectOpenHandles --forceExit --config ./src/__tests__/smoketest/jest.config.js",
    "proto:test": "env NODE_ENV=development /Users/neb_401/.nvm/versions/node/v10.15.3/bin/node -r ts-node/register /Users/neb_401/Developer/neb401corp/LuxciumProject/questrade-ts/src/proto_testing/main.ts",
    "debug": "cd /Users/neb_401/Developer/neb401corp/LuxciumProject/questrade ; env NODE_ENV=development /Users/neb_401/.nvm/versions/node/v10.15.3/bin/node -r ts-node/register --inspect=9922 /Users/neb_401/Developer/neb401corp/LuxciumProject/questrade/src/main.ts",
    "gitcheck": "git diff-index origin/master",
    "copy:package:json": "cp-cli package.json ./build/package.json",
    "copy:npmignore": "cp-cli .npmignore ./build/.npmignore",
    "copy:LICENSE:notice": "cp-cli notice.md build/notice.md",
    "copy:LICENSE": "cp-cli LICENSE ./build/LICENSE && npm run copy:LICENSE:notice",
    "copy:readme": "cp-cli README.md build/README.md",
    "copy:static:files": "npm run copy:package:json && npm run copy:npmignore && npm run copy:LICENSE && npm run copy:readme",
    "predist:install": "rimraf package-lock.json && rimraf node_modules && npm install && npm i -D @types/node@latest",
    "predist:check": "npm run proto:test && echo 'will smoke:test' && echo 'will test'",
    "quick:predist": "npm run gitcheck && npm run build && npm run copy:static:files && rimraf ./build/tmp",
    "predist": "npm run predist:install && npm run quick:predist && echo done",
    "dist": "cd build && npm publish",
    "quick:dist": "npm run quick:predist && echo 'all good'",
    "lint": "tslint --project . --format stylish",
    "pretest": "tsc --noEmit --pretty && npm run lint && npm run smoke:test",
    "test": "echo 'will test",
    "release:minor": "npm run predist:check && npm version minor && npm run dist",
    "release:patch": "npm run predist:check && npm version patch && npm run dist",
    "release:again": "npm run predist:check && npm run dist",
    "release": "npm run release:again "
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/luxcium/questrade-ts.git"
  },
  "keywords": ["Questrade", "stocks", "trading", "typescript"],
  "author": "Luxcium <luxcium@neb401.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luxcium/questrade-ts/issues"
  },
  "homepage": "https://github.com/luxcium/questrade-ts",
  "dependencies": {
    "axios": "0.19.0",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@types/node": "12.6.8",
    "concurrently": "4.1.2",
    "coveralls": "^3.0.6",
    "cp-cli": "2.0.0",
    "prettier": "1.18.2",
    "rimraf": "2.6.3",
    "ts-node": "8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.5.3",
    "typescript-tslint-plugin": "0.5.4"
  }
}
