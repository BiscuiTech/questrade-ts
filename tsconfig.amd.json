{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "amd",
    "outFile": "./build/js/questrade.amd.js",
    "tsBuildInfoFile": "./build/tmp/tsbuild.amd.info",
    "noEmit": false,
    "noEmitHelpers": false,
    /* Additional Vebosity */
    "diagnostics": true,
    "extendedDiagnostics": false,
    "listFiles": false,
    "listEmittedFiles": false,
    "traceResolution": false,
    /* Source Map Options */
    "composite": false,
    "declaration": false,
    "declarationMap": false,
    "sourceMap": false,
    "inlineSourceMap": false,
    "inlineSources": false
  }
}
