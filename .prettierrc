{
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "embeddedLanguageFormatting": "auto",
  "endOfLine": "lf",
  "htmlWhitespaceSensitivity": "css",
  "insertPragma": false,
  "jsxBracketSameLine": false,
  "jsxSingleQuote": false,
  "overrides": [
    {
      "files": ".prettierrc",
      "options": {
        "parser": "json",
        "trailingComma": "all"
      }
    }
  ],
  "parser": "typescript",
  "printWidth": 20,
  "proseWrap": "preserve",
  "quoteProps": "consistent",
  "rangeStart": 0,
  "requirePragma": false,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false
}
